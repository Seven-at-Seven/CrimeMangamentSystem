<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.sevenatseven.controllers.auth.AdminLoginController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox spacing="10" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10">
        <!-- Breadcrumb -->
        <HBox spacing="5" alignment="CENTER_LEFT">
            <Hyperlink onAction="#goToHome" text="Home" />
            <Label text=">" />
            <Label text="Admin Login" />
            <padding>
                <Insets bottom="20" />
            </padding>
        </HBox>

        <!-- Main Content -->
        <Label fx:id="label" text="Login Scene" />

    </VBox>

</AnchorPane>
